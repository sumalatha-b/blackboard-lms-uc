<?xml version="1.0" encoding="UTF-8"?>

<mule xmlns:ee="http://www.mulesoft.org/schema/mule/ee/core" xmlns:blackboard="http://www.mulesoft.org/schema/connectivity/blackboard"
	xmlns="http://www.mulesoft.org/schema/mule/core"
	xmlns:doc="http://www.mulesoft.org/schema/mule/documentation" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.mulesoft.org/schema/mule/core http://www.mulesoft.org/schema/mule/core/current/mule.xsd
http://www.mulesoft.org/schema/connectivity/blackboard http://www.mulesoft.org/schema/connectivity/blackboard/current/mule-blackboard.xsd
http://www.mulesoft.org/schema/mule/ee/core http://www.mulesoft.org/schema/mule/ee/core/current/mule-ee.xsd">
	<sub-flow name="accesstoken_Flow" doc:id="7e872312-7bc8-4b60-b9ea-dde7ca788447" >
		<logger level="INFO" doc:name="Logger" doc:id="aaf07201-be82-4f3a-8803-4e507cabd5ae" />
		<ee:transform doc:name="Transform Message" doc:id="779e8599-2339-4979-861d-690d6fe2e201" >
			<ee:message >
				<ee:set-payload ><![CDATA[%dw 2.0
output application/java
---
{
	"grant_type": "client_credentials"
}]]></ee:set-payload>
			</ee:message>
		</ee:transform>
		<blackboard:access-token doc:name="Token - AccessToken" doc:id="3ee0ac51-0eb4-4963-8e97-b8044c9a04a5" config-ref="Token_Blackboard_config" correlationId="#[correlationId]">
			<blackboard:token_-_access-token--body ><![CDATA[#[payload]]]></blackboard:token_-_access-token--body>
		</blackboard:access-token>
		<logger level="INFO" doc:name="Logger" doc:id="148af11d-dd5e-4fb3-b77b-520794a2643a" />
	</sub-flow>
</mule>
